{
  "version": "2.0.0",
  "tasks": [
      {
          "label": "Incremental Build",
          "type": "shell",
          "command": "powershell",
          "args": [
              "-NoProfile",
              "-ExecutionPolicy", "Bypass",
              "-File",
              "${workspaceFolder}/build.ps1"
          ],
          "problemMatcher": ["$msCompile"],
          "group": { "kind": "build" }
      }
  ]
}
